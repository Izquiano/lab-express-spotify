<h2>Abums for: {{albumsData.[0].artists.[0].name}}</h2>
<div class="container-albums">
    {{#each albumsData }}
    <div class="album">
        <img class="image" src="{{images.[0].url}}" alt="image {{name}}" />
        <div>
            <h6>{{name}}</h6>
            <a class="btn" href="/tracks/{{id}}">View Tracks</a>
        </div>
    </div>
    {{/each}}
</div>